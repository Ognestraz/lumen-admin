{
    "name": "ognestraz/lumen-admin",
    "description": "The Admin for Laravel Lumen Framework.",
    "keywords": ["admin", "framework", "laravel", "lumen"],
    "authors": [
        {
            "name": "ognestraz",
            "email": "ognestraz@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "intervention/image": "~2.1",
        "vlucas/phpdotenv": "~1.0",
        "vluzrmos/collective-html": "^1.0",
        "illuminate/html":"5.0.*",
        "league/flysystem": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],        
        "psr-4": {
            "Admin\\": "app/",
            "Model\\": "model/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
